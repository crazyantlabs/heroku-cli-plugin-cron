{
  "name": "heroku-cron",
  "version": "1.0.8",
  "description": "Heroku CLI plugin for Cron To Go",
  "author": "Moty Michaely @motymichaely",
  "bin": {
    "heroku-cli-plugin-cron": "bin/run"
  },
  "homepage": "https://github.com/crazyantlabs/heroku-cli-plugin-cron",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyantlabs/heroku-cli-plugin-cron.git"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^8.5.0",
    "@heroku-cli/schema": "^1.0.25",
    "@heroku/eventsource": "^1.0.7",
    "@oclif/command": "^1",
    "@oclif/config": "^1.18.3",
    "@oclif/core": "^1",
    "@oclif/errors": "^1.3.5",
    "@oclif/help": "^1.0.1",
    "@oclif/plugin-plugins": "^2.1.0",
    "cron-parser": "^4.5.0",
    "fs-extra": "^10.0.1",
    "http-call": "^5.3.0",
    "inquirer": "^8.2.4",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/eventsource": "^1.1.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.9.4",
    "@types/validator": "^13.7.3",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eventsource": "^2.0.2",
    "globby": "^11",
    "mocha": "^9",
    "np": "*",
    "nyc": "^15.1.0",
    "oclif": "^2",
    "shx": "^0.3.4",
    "sinon": "^13.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "heroku",
    "dirname": "heroku-cli-plugin-cron",
    "commands": "./lib/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topicSeparator": ":",
    "topics": {
      "cron": {
        "description": "manage Cron To Go resources"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "nyc --extension .ts mocha --trace-warnings --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "release": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/crazyantlabs/heroku-cli-plugin-cron/issues"
  },
  "keywords": [
    "heroku",
    "heroku-cli-plugin",
    "crazyantlabs",
    "cron-to-go"
  ],
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
